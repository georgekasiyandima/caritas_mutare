{
  "name": "caritas-mutare",
  "version": "1.0.0",
  "description": "Digital platform for Caritas Mutare - Catholic community development organization",
  "main": "server/index.js",
  "scripts": {
    "dev": "fkill :5000 :3000 -f -s && concurrently \"npm:backend\" \"npm:frontend\"",
    "test": "jest --watchAll --verbose",
    "build": "cross-env NODE_ENV=production npm run build:client",
    "build:client": "cd client && npm run build",
    "frontend": "cd client && npm start",
    "backend": "cd server && npm run dev",
    "start": "npm run backend",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start"
  },
  "keywords": [
    "caritas",
    "mutare",
    "catholic",
    "community-development",
    "nonprofit",
    "zimbabwe"
  ],
  "author": "Ubuntu Tech and Code the Dream  Development Team",
  "license": "MIT",
